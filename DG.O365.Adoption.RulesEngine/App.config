<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.0" />
  </startup>
  <connectionStrings>
    <add name="SqlConnectionString" connectionString="Server={yourserver};Initial Catalog={yourcatalog};Persist Security Info=False;User ID={username};Password={password};MultipleActiveResultSets=False;Encrypt=True;TrustServerCertificate=False;Connection Timeout=30;" />
  </connectionStrings>
  <appSettings>
    <add key="AzureConnectionString" value=""/>
    <add key="NotificationURI" value="https://dgadoptionbot.azurewebsites.net/api/start"/>
    <add key="TimerJobInterval" value="86400000"/> <!--milliseconds-->
  </appSettings>
</configuration>